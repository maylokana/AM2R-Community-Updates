            <string>if (oControl.kRightPressed){
    sfx_play(sndMenuMove);
    ++option;
    if (option &gt; 2) then option = 0;
}

if (oControl.kLeftPressed){
    sfx_play(sndMenuMove);
    --option;
    if (option &lt; 0) then option = 2;
}

if (oControl.kMenu1Pressed){
    audio_stop_all();
    sfx_play(sndMenuSel);
    switch (option){
        case 0:
            start_trial();
        break;
        case 1:
            save_demo();
        break;
        case 2:
            room_change(trialselectroom,1);
        break;
    }
}

_hue += 0.33;
if (_hue &gt; 255) then _hue -= 255;

_stx += 0.25;
if (_stx &gt; 240) then _stx -= 240;
